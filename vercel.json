{
  "version": 2,
  "builds": [
    { "src": "website/**", "use": "@vercel/static" },
    { "src": "assets/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "^/$", "dest": "/website/p√°ginas/index.html" },
    { "src": "^/assets/(.*)$", "dest": "/assets/$1" },
    { "src": "^/styles/(.*)$", "dest": "/website/styles/$1" },
    { "src": "^/scripts/(.*)$", "dest": "/website/scripts/$1" },
    { "src": "^/(.*)$", "dest": "/website/$1" }
  ]
}
